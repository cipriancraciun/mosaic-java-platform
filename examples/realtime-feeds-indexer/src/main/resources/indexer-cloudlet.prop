
cloudlet.main_class=eu.mosaic_cloud.examples.realtime_feeds.indexer.IndexerCloudlet$LifeCycleHandler
cloudlet.context_class=eu.mosaic_cloud.examples.realtime_feeds.indexer.IndexerCloudlet$IndexerCloudletContext
cloudlet.resource_file=indexer-cloudlet.prop

urgent.queue.consumer.amqp.exchange=feeds.index-data
urgent.queue.consumer.amqp.exchange_type=topic
urgent.queue.consumer.amqp.routing_key=urgent
urgent.queue.consumer.amqp.queue=feeds.index-data.urgent
urgent.queue.consumer.amqp.durable=true
urgent.queue.consumer.amqp.auto_delete=false
urgent.queue.consumer.amqp.passive=false
urgent.queue.consumer.amqp.exclusive=false
urgent.queue.consumer.amqp.auto_ack=false

batch.queue.consumer.amqp.exchange=feeds.index-data
batch.queue.consumer.amqp.exchange_type=topic
batch.queue.consumer.amqp.routing_key=batch
batch.queue.consumer.amqp.queue=feeds.index-data.batch
batch.queue.consumer.amqp.durable=true
batch.queue.consumer.amqp.auto_delete=false
batch.queue.consumer.amqp.passive=false
batch.queue.consumer.amqp.exclusive=false
batch.queue.consumer.amqp.auto_ack=false

metadata.kvstore.bucket=feed-metadata

timelines.kvstore.bucket=feed-timelines

items.kvstore.bucket=feed-items

data.kvstore.bucket=feed-data

tasks.kvstore.bucket=feed-tasks
